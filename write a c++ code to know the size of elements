#include <iostream>
using namespace std;

int main() {
    int i, n;
    
    cout << "Enter size of the array:" << endl;
    cin >> n;
    
    int x[n]; 
    
    cout << "Enter the elements of the array:" << endl;
    for (i = 0; i < n; i++) {  
        cin >> x[i];
    }
    
    cout << "The elements of the array are:" << endl;
    for (i = 0; i < n; i++) {
        cout << x[i] << " ";
    }
    cout << endl;
    
    return 0;
}
